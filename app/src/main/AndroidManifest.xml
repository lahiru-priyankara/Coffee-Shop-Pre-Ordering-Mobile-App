        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LabExam2"
        tools:targetApi="31">


        <activity
            android:name=".Launch_Screen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Onboarding_Screen_1" />
        <activity android:name=".Onboarding_Screen_2" />
        <activity android:name=".Onboarding_Screen_3" />
        <activity android:name=".Sign_In" />
        <activity android:name=".Sign_Up" />
